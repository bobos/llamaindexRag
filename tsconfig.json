{
  "compilerOptions":
    {
      "target": "es2017",
      "module": "commonjs",
      "outDir": "./build",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true, /* Enable all strict type checking */
      "skipLibCheck": true    /* Skip type checking all .d.ts files*/
    },
  "include": [
    "src/**/*.ts"
  ]
}